            <table cellpadding="0" cellspacing="0" border="0" height="100%" align="center" valign="top" class="text-left" style="max-width:600px; width:100%; margin:0 auto;">
              <tr>
                <td class="body-container">
                  block content
                </td>
              </tr>
            </table>
            //- End table content
